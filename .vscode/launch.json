{
    "version": "1.0.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "console": "integratedTerminal",
            "name": "Launch Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/index.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}